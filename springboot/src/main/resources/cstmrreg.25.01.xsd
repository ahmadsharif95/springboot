<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="urn:ats:mpc:cstmrreg.25.01" elementFormDefault="qualified"
           targetNamespace="urn:ats:mpc:cstmrreg.25.01" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Document">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="IsDefAcctReq">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="GrpHdr">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="MsgId" type="tns:Max35Text"/>
                                        <xs:element name="CreDtTm" type="xs:dateTime"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="PrtcpntId" type="tns:Max35Text" />
                            <xs:element name="MblOrSvc" type="tns:Max35Text"/>
                            <xs:element name="RgstrtnCd" type="tns:Max10Text" minOccurs="0"/>
                            <xs:element name="AcntTp" type="tns:Max1Text" minOccurs="0"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="Max10Text">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Max1Text">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Max35Text">
        <xs:restriction base="xs:string">
            <xs:minLength value="0"/>
            <xs:maxLength value="35"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>